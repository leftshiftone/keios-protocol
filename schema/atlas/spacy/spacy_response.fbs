namespace keios.atlas.spacy;

attribute "priority";

table DEPSpacyResponse {
    lang: string;
    relation: string;
    source: string;
    sourcePos: string;
    sourceIndex: int;
    sourceTag: string;
    sourceBase: string;
    target: string;
    targetPos: string;
    targetIndex: int;
    targetTag: string;
    targetBase: string;
}

